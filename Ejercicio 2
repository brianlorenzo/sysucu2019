//Onda Triangular
t1=0:0.000125:0.024875;
t2=0.025:0.000125:0.049875;
y1=200*t1;
y2=-200*t2+10;
sd2=[y1 y2];

function y=codificador(x)
    z=strsplit(strcat(dec2bin(ascii(x))))'
    a=0
    for i=1:size(z)(2)
        if z(i)=='1' then
            a=[a,sd2]
        else
            a=[a,-sd2]
        end
    end
    y=a
endfunction
